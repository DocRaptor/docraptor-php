{
    "name": "docraptor/docraptor",
    "description": "A wrapper for the DocRaptor HTML to PDF/XLS service.",
    "keywords": [
        "docraptor",
        "api",
        "pdf",
        "xls",
        "xlsx",
        "html"
    ],
    "homepage": "https://github.com/docraptor/docraptor-php",
    "license": "Apache v2",
    "authors": [
        {
            "name": "Elijah Miller",
            "homepage": "https://github.com/jqr"
        }
    ],
    "require": {
        "php": ">=5.3.3",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "satooshi/php-coveralls": "~0.6.1",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": { "DocRaptor\\" : "lib/" }
    }
}
